<?xml version="1.0" encoding="utf-8"?>
<com.flipboard.bottomsheet.BottomSheetLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/record_bottomsheet"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:id="@+id/b31Coordinator"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true">

        <com.guider.healthring.widget.NoScrollViewPager
            android:id="@+id/b31View_pager"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/bottomBar"
            app:layout_behavior="@string/appbar_scrolling_view_behavior" />


        <com.guider.healthring.widget.BottomSelectView
            android:id="@+id/b31BottomBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"
            android:background="#fff" />
        <!--<com.roughike.bottombar.BottomBar-->
        <!--android:id="@+id/b31BottomBar"-->
        <!--android:visibility="gone"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="60dp"-->
        <!--android:layout_gravity="bottom"-->
        <!--app:bb_tabXmlResource="@xml/b30_bottom_tabs" />-->
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</com.flipboard.bottomsheet.BottomSheetLayout>

