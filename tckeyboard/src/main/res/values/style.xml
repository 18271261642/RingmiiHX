<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr format="boolean" name="isRandom"/>
    <attr format="reference" name="keyDrawable"/>
    <attr format="reference" name="xmlLayoutResId"/>
    <style name="AnimationFade">
        <item name="android:windowEnterAnimation">@anim/dialog_enter</item>
        <item name="android:windowExitAnimation">@anim/dialog_exit</item>
    </style>
    <declare-styleable name="SystemKeyBoardEditText">
        <attr name="xmlLayoutResId"/>
        <attr name="keyDrawable"/>
        <attr format="boolean" name="outSideCancel"/>
        <attr format="boolean" name="space"/>
        <attr name="isRandom"/>
    </declare-styleable>
    <declare-styleable name="SystemKeyboard">
        <attr name="xmlLayoutResId"/>
        <attr name="keyDrawable"/>
        <attr name="isRandom"/>
    </declare-styleable>
</resources>