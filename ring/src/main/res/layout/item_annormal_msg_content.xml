<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="6dp"
    android:layout_height="wrap_content">
    <TextView
        android:layout_width="69dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:text="@string/app_msg_item_suggest"
        android:textColor="@color/color999999"
        android:textSize="@dimen/sp_13"
        android:id="@+id/dataTitle"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/dataTitle"
        app:layout_constraintTop_toTopOf="@id/dataTitle"
        android:layout_marginStart="5dp"
        android:text="139 mmHg"
        android:textColor="#ff333333"
        android:textSize="@dimen/sp_13"
        android:id="@+id/dataContentTv"
        />
    <ImageView
        android:layout_width="9dp"
        android:layout_height="12dp"
        app:layout_constraintStart_toEndOf="@id/dataContentTv"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginStart="5dp"
        android:id="@+id/dataStatusIv"
        android:visibility="gone"
        />
</androidx.constraintlayout.widget.ConstraintLayout>