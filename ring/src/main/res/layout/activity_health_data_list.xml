<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="13dp"
        android:paddingEnd="13dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="14dp"
        android:layout_marginTop="7dp"
        android:background="@drawable/rounded_f5f5f5_14_bg"
        android:text="2019-07-13"
        android:textColor="@color/color999999"
        android:textSize="@dimen/sp_13"
        android:id="@+id/startTimeTv"
        />
    <TextView
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/startTimeTv"
        app:layout_constraintTop_toTopOf="@id/startTimeTv"
        app:layout_constraintBottom_toBottomOf="@id/startTimeTv"
        android:text="-"
        android:textColor="@color/colorDDDDDD"
        android:textSize="@dimen/sp_13"
        android:layout_marginStart="5dp"
        android:id="@+id/timeLine"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="13dp"
        android:paddingEnd="13dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        app:layout_constraintStart_toEndOf="@id/timeLine"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="5dp"
        android:layout_marginTop="7dp"
        android:background="@drawable/rounded_f5f5f5_14_bg"
        android:text="2019-07-13"
        android:textColor="@color/color999999"
        android:textSize="@dimen/sp_13"
        android:id="@+id/endTimeTv"
        />
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="71dp"
        android:layout_height="29dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="13dp"
        android:id="@+id/searchLayout"
        android:background="@drawable/rounded_f28a38_14_bg"
        >
        <ImageView
            android:layout_width="17dp"
            android:layout_height="14dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="12dp"
            android:id="@+id/searchIv"
            android:src="@drawable/icon_search_white"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@id/searchIv"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:text="@string/app_main_health_query"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_13"
            android:layout_marginStart="2dp"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0_5"
        app:layout_constraintTop_toBottomOf="@id/startTimeTv"
        android:layout_marginTop="7dp"
        android:id="@+id/line"
        android:background="@color/colorDDDDDD"
        />
    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/line"
        android:overScrollMode="never"
        android:id="@+id/healthDataListRv"
        />
</androidx.constraintlayout.widget.ConstraintLayout>