<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape
            android:shape="rectangle">
            <solid android:color="#ffffff" />
            <stroke android:width="@dimen/dp_1" android:color="#F2F2F2" />
            <corners android:bottomLeftRadius="@dimen/dp_5" android:bottomRightRadius="@dimen/dp_5" android:topLeftRadius="@dimen/dp_5" android:topRightRadius="@dimen/dp_5" />

        </shape>
    </item>

    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="#ffffff" />
            <stroke android:width="@dimen/dp_1" android:color="#F18937" />
            <corners android:bottomLeftRadius="@dimen/dp_5" android:bottomRightRadius="@dimen/dp_5" android:topLeftRadius="@dimen/dp_5" android:topRightRadius="@dimen/dp_5" />
        </shape>
    </item>
</selector>




