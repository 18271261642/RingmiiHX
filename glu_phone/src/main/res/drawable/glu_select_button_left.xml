<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dp_1" android:color="#F18937" />
            <solid android:color="#FFFFFF"></solid>
            <corners android:bottomLeftRadius="@dimen/dp_22"
                android:bottomRightRadius="@dimen/dp_0"
                android:topLeftRadius="@dimen/dp_22"
                android:topRightRadius="@dimen/dp_0" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dp_1" android:color="#F18937" />
            <solid android:color="#F18937"></solid>
            <corners android:bottomLeftRadius="@dimen/dp_22"
                android:bottomRightRadius="@dimen/dp_0"
                android:topLeftRadius="@dimen/dp_22"
                android:topRightRadius="@dimen/dp_0" />
        </shape>
    </item>
</selector>